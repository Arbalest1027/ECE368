#include <stdio.h>
#include <stdlib.h>
#include <string.h>

int main(int argc, char ** argv){
	int * A = malloc(sizeof(int) * 5);
	for (int i = 0 i < 5;i++){
	 	A[i] = 
	int d = 3;
	int n = strlen(A);
	left_rotate(A,n,d);
	return EXIT_SUCCESS;
}

void left_rotate(int * A, int n, int d){
	for (int i = 0;i < d;i++){
		int temp = A[0];
		for (int i = 0;i < n -1;i++){
			A[i] = A[i + 1];
			A[i] = temp;
		}
	}
}
